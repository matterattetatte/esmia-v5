---
eleventyNavigation:
  key: "Start"
  order: 1
  url: "/#"
title: "Start"
---
{% block content %}
  {% set navItems = collections.all | eleventyNavigation | sort(false, false, "order") %}
{% for item in navItems | head(6) %}
    <section id="{{ item.key | slug }}">
      {% if item.key != "Start" %}
        {% set entry = collections.all | findByUrl(item.url) %}
        {% if entry %}
          {{ entry.templateContent | safe }}
        {% else %}
          <p>Fel: Innehåll kunde inte hittas för URL {{ item.url }}</p>
        {% endif %}
      {% else %}
<style>{% include "css/banner.css" %}</style>
<div class="banner-section">
  <div class="banner-image">
    <img src="/images/background.jpg" alt="Spansk cateringillustration" />
  </div>
</div>

  <center class="banner-text">
    <h1>ESMIA HB</h1>
    <p>Spansk paella-och tapascatering i Stockholm</p>
  </center>

      {% endif %}
    </section>
  {% endfor %}

{% endblock %}